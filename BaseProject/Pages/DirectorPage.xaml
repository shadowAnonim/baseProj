<Page x:Class="BaseProject.Pages.DirectorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BaseProject.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Меню директора">

    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <StackPanel>
            <Button x:Name="usersBtn" Content="Пользователи" Click="usersBtn_Click"/>
            <Button x:Name="projectBtn" Content="Проекты" Click="projectBtn_Click"/>
            <Button x:Name="graphBtn" Content="График" Click="graphBtn_Click"/>
        </StackPanel>
    </Grid>
</Page>
